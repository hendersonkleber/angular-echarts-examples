{
  "version": 1,
  "themeName": "dark",
  "theme": {
    "color": ["#a3e635", "#fbbf24", "#dc2626", "#22d3ee", "#8b5cf6", "#d946ef", "#ec4899"],

    "backgroundColor": "rgba(38,38,38, 0)",
    "textStyle": {
      "color": "#ffffff",
      "fontFamily": "'Roboto', 'Helvetica Neue', 'sans-serif'"
    },

    "darkMode": true,

    "title": {
      "textStyle": {
        "color": "#eeeeee"
      },
      "subtextStyle": {
        "color": "#aaaaaa"
      }
    },

    "line": {
      "itemStyle": {
        "borderWidth": "2"
      },
      "lineStyle": {
        "width": "4"
      },
      "symbolSize": "8",
      "symbol": "emptyCircle",
      "smooth": true
    },

    "radar": {
      "itemStyle": {
        "borderWidth": "2"
      },
      "lineStyle": {
        "width": "4"
      },
      "symbolSize": "8",
      "symbol": "emptyCircle",
      "smooth": true
    },

    "bar": {
      "itemStyle": {
        "barBorderWidth": 0,
        "barBorderColor": "rgba(38,38,38, 0)"
      }
    },

    "pie": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(38,38,38, 0)"
      },
      "emphasis": {
        "itemStyle": {
          "borderColor": "rgba(38,38,38, 0)",
          "borderWidth": 0
        },
        "label": {
          "color": "#eeeeee"
        }
      }
    },

    "scatter": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(38,38,38, 0)"
      }
    },

    "boxplot": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(38,38,38, 0)"
      }
    },

    "parallel": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(38,38,38, 0)"
      }
    },

    "sankey": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(38,38,38, 0)"
      }
    },

    "funnel": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(38,38,38, 0)"
      }
    },

    "gauge": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(38,38,38, 0)"
      },

      "progress": {
        "itemStyle": {
          "color": "#a3e635"
        }
      },

      "axisLine": {
        "lineStyle": {
          "color": [[1, "rgba(163, 230, 53, 0.2)"]]
        }
      },

      "detail": {
        "color": "#eeeeee"
      }
    },

    "candlestick": {
      "itemStyle": {
        "color": "#eb5454",
        "color0": "#47b262",
        "borderColor": "#eb5454",
        "borderColor0": "#47b262",
        "borderWidth": 1
      }
    },

    "graph": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "#262626"
      },
      "lineStyle": {
        "width": 1,
        "color": "#aaaaaa"
      },
      "symbolSize": "8",
      "symbol": "emptyCircle",
      "smooth": true,
      "color": ["#a3e635", "#fbbf24", "#dc2626", "#22d3ee", "#8b5cf6", "#d946ef", "#ec4899"],
      "label": {
        "color": "#eeeeee"
      }
    },

    "map": {
      "itemStyle": {
        "areaColor": "#a8a29e",
        "borderColor": "#444444",
        "borderWidth": 0.5
