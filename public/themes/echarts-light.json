{
  "version": 1,
  "themeName": "light",
  "theme": {
    "color": ["#a3e635", "#fbbf24", "#dc2626", "#22d3ee", "#8b5cf6", "#d946ef", "#ec4899"],

    "backgroundColor": "rgba(255,255,255,0)",

    "textStyle": {
      "color": "rgba(0, 0, 0, 0.87)",
      "fontFamily": "'Roboto', 'Helvetica Neue', 'sans-serif'"
    },

    "darkMode": false,

    "title": {
      "textStyle": {
        "color": "#464646"
      },
      "subtextStyle": {
        "color": "#6e7079"
      }
    },

    "line": {
      "itemStyle": {
        "borderWidth": "2"
      },
      "lineStyle": {
        "width": "4"
      },
      "symbolSize": "8",
      "symbol": "emptyCircle",
      "smooth": true
    },

    "radar": {
      "itemStyle": {
        "borderWidth": "2"
      },
      "lineStyle": {
        "width": "4"
      },
      "symbolSize": "8",
      "symbol": "emptyCircle",
      "smooth": true
    },

    "bar": {
      "itemStyle": {
        "barBorderWidth": 0,
        "barBorderColor": "rgba(255,255,255,0)"
      },
      "tooltip": {
        "borderColor": "#e5e5e5",
        "backgroundColor": "#ffffff",
        "textStyle": {
          "color": "rgba(0, 0, 0, 0.87)"
        }
      }
    },

    "pie": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(255,255,255,0)"
      },
      "emphasis": {
        "itemStyle": {
          "borderColor": "rgba(255,255,255,0)",
          "borderWidth": 0
        },
        "label": {
          "color": "#464646"
        }
      }
    },

    "scatter": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(255,255,255,0)"
      }
    },

    "boxplot": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(255,255,255,0)"
      }
    },

    "parallel": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(255,255,255,0)"
      }
    },

    "sankey": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(255,255,255,0)"
      }
    },

    "funnel": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(255,255,255,0)"
      }
    },

    "gauge": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "rgba(255,255,255,0)"
      },

      "progress": {
        "itemStyle": {
          "color": "#a3e635"
        }
      },

      "axisLine": {
        "lineStyle": {
          "color": [[1, "rgba(163, 230, 53, 0.2)"]]
        }
      },

      "detail": {
        "color": "#464646"
      }
    },

    "candlestick": {
      "itemStyle": {
        "color": "#eb5454",
        "color0": "#47b262",
        "borderColor": "#eb5454",
        "borderColor0": "#47b262",
        "borderWidth": 1
      }
    },

    "graph": {
      "itemStyle": {
        "borderWidth": 0,
        "borderColor": "#fafafa"
      },

      "lineStyle": {
        "width": 1,
        "color": "#aaaaaa"
      },

      "symbolSize": "8",
      "symbol": "emptyCircle",
      "smooth": true,
      "color": ["#a3e635", "#fbbf24", "#dc2626", "#22d3ee", "#8b5cf6", "#d946ef", "#ec4899"],
      "label": {
        "color": "#eeeeee"
      }
    },

    "map": {
      "itemStyle": {
        "areaColor": "#eeeeee",
        "borderColor": "#444444",
        "borderWidth": 0.5
      },
      "label": {
        "color": "#000000"
      },
      "emphasis": {
        "itemStyle": {
          "areaColor": "#a3e635",
          "borderColor": "#444444",
          "borderWidth": 1
        },
        "label": {
          "color": "#000000"
        }
      }
    },

    "geo": {
      "itemStyle": {
        "areaColor": "#eeeeee",
        "borderColor": "#444444",
        "borderWidth": 0.5
      },
      "label": {
        "color": "#000000"
      },
      "emphasis": {
        "itemStyle": {
          "areaColor": "#a3e635",
          "borderColor": "#444444",
          "borderWidth": 1
        },
        "label": {
          "color": "#000000"
        }
      }
    },

    "categoryAxis": {
      "axisPointer": {
        "label": {
          "borderColor": "#e5e5e5",
          "borderWidth": 1,
          "backgroundColor": "#ffffff",
          "color": "rgba(0, 0, 0, 0.87)"
        }
      },

      "axisLine": {
        "show": true,
        "lineStyle": {
          "color": "#6E7079"
        }
      },

      "axisTick": {
        "show": true,
        "lineStyle": {
          "color": "#6E7079"
        }
      },

      "axisLabel": {
        "show": true,
        "color": "#6E7079"
      },

      "splitLine": {
        "show": true,
        "lineStyle": {
          "color": ["#E0E6F1"]
        }
      },

      "splitArea": {
        "show": false,
        "areaStyle": {
          "color": ["rgba(250,250,250,0.2)", "rgba(210,219,238,0.2)"]
        }
      }
    },

    "valueAxis": {
      "axisPointer": {
        "label": {
          "borderColor": "#e5e5e5",
          "borderWidth": 1,
          "backgroundColor": "#ffffff",
          "color": "rgba(0, 0, 0, 0.87)"
        }
      },

      "axisLine": {
        "show": true,
        "lineStyle": {
          "color": "#6E7079"
        }
      },

      "axisTick": {
        "show": true,
        "lineStyle": {
          "color": "#6E7079"
        }
      },

      "axisLabel": {
        "show": true,
        "color": "#6E7079"
      },

      "splitLine": {
        "show": true,
        "lineStyle": {
          "color": ["#E0E6F1"]
        }
      },

      "splitArea": {
        "show": false,
        "areaStyle": {
          "color": ["rgba(250,250,250,0.2)", "rgba(210,219,238,0.2)"]
        }
      }
    },

    "logAxis": {
      "axisLine": {
        "show": true,
        "lineStyle": {
          "color": "#6E7079"
        }
      },
      "axisTick": {
        "show": true,
        "lineStyle": {
          "color": "#6E7079"
        }
      },
      "axisLabel": {
        "show": true,
        "color": "#6E7079"
      },
      "splitLine": {
        "show": true,
        "lineStyle": {
          "color": ["#E0E6F1"]
        }
      },
      "splitArea": {
        "show": false,
        "areaStyle": {
          "color": ["rgba(250,250,250,0.2)", "rgba(210,219,238,0.2)"]
        }
      }
    },

    "timeAxis": {
      "axisLine": {
        "show": true,
        "lineStyle": {
          "color": "#6E7079"
        }
      },
      "axisTick": {
        "show": true,
        "lineStyle": {
          "color": "#6E7079"
        }
      },
      "axisLabel": {
        "show": true,
        "color": "#6E7079"
      },
      "splitLine": {
        "show": true,
        "lineStyle": {
          "color": ["#E0E6F1"]
        }
      },
      "splitArea": {
        "show": false,
        "areaStyle": {
          "color": ["rgba(250,250,250,0.2)", "rgba(210,219,238,0.2)"]
