    "toolbox": {
      "iconStyle": {
        "borderColor": "#999999"
      },
      "emphasis": {
        "iconStyle": {
          "borderColor": "#666666"
        }
      }
    },

    "legend": {
      "textStyle": {
        "color": "#333333"
      }
    },

    "tooltip": {
      "borderColor": "#e5e5e5",
      "backgroundColor": "#ffffff",
      "textStyle": {
        "color": "rgba(0, 0, 0, 0.87)"
